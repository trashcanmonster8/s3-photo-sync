{
  "name": "reducer",
  "version": "1.0.0",
  "description": "Lambda for reducing image size",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./helpers/*.js ./index.js",
    "format": "npm run lint:fix && npm run prettier:fix",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "npm prune --production && zip -r function.zip package.json index.js ./node_modules",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier -w .",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trashcanmonster8/s3-photo-sync.git"
  },
  "keywords": [
    "Lambda",
    "AWS",
    "image"
  ],
  "author": "Jacob Mayer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trashcanmonster8/s3-photo-sync/issues"
  },
  "homepage": "https://github.com/trashcanmonster8/s3-photo-sync#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.84",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^9.1.3",
    "nock": "^13.1.4",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.38.0",
    "@aws-sdk/protocol-http": "^3.38.0",
    "@aws-sdk/s3-request-presigner": "^3.39.0",
    "aws-lambda": "^1.0.6",
    "jimp": "^0.16.1"
  }
}
